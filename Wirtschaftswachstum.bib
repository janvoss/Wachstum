	author = {Aghion, Philippe and Howitt, Peter W.},
	date = {2008},
	langid = {english},
	note = {Google-Books-{ID}: B9vxCwAAQBAJ},
	keywords = {Business \& Economics / Development / Economic Development, Business \& Economics / Economics / Comparative, Business \& Economics / Government \& Business},
}

